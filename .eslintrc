{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "amd": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 1,
    "curly": 1,
    "default-case": 1,
    "eqeqeq": 1,
    "no-alert": 0,
    "no-cond-assign": 1,
    "no-console": 0,
    "no-dupe-args": 0,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-eval": 1,
    "no-extra-semi": 0,
    "no-floating-decimal": 1,
    "no-loop-func": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "quotes": [ 1, "single" ],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "semi": [1, "never" ],
    "spaced-comment":1,
    "strict": 0,
    "use-isnan": 1,
    "vars-on-top": 1
  }
}
