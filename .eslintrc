{
  "parser": "babel-eslint",
  "plugins": [
  ],
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "__DEV__": true
  },
  "env": {
    "amd": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "array-bracket-spacing": [
      1,
      "never"
    ],
    "comma-dangle": [
      1,
      "never"
    ],
    "curly": 1,
    "default-case": 1,
    "eqeqeq": 1,
    "no-alert": 0,
    "no-cond-assign": 1,
    "no-console": 0,
    "no-dupe-args": 0,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-eval": 1,
    "no-extra-semi": 0,
    "no-floating-decimal": 1,
    "no-loop-func": 1,
    "no-trailing-spaces": [
      1,
      {
        "skipBlankLines": true
      }
    ],
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "object-curly-spacing": [
      1,
      "never"
    ],
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      1,
      "never"
    ],
    "space-before-function-paren": 1,
    "space-in-parens": [
      1,
      "never"
    ],
    "spaced-comment": 1,
    "strict": 1,
    "use-isnan": 1,
    "vars-on-top": 1
  }
}
